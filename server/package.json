{
  "type": "module",
  "name": "sf-lifeline-server",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "version": "1.0.0",
  "main": "app.js",
  "directories": {
    "test": "test"
  },
  "prisma": {
    "seed": "tsx prisma/seed"
  },
  "scripts": {
    "test": "node --test test/**/*.test.js",
    "start": "fastify start -l info app.js",
    "dev": "npx prisma migrate dev && fastify start -w -l info -P app.js",
    "migrate": "docker-compose exec server sh -c 'cd server && npx prisma migrate $@' sh",
    "db": "docker-compose exec server sh -c 'cd server && npx prisma db $@' sh",
    "pretest": "standard",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "^5.0.0",
    "@fastify/sensible": "^5.0.0",
    "@prisma/client": "^5.9.1",
    "fastify": "^4.0.0",
    "fastify-cli": "^6.0.1",
    "fastify-plugin": "^4.0.0"
  },
  "devDependencies": {
    "c8": "^7.13.0",
    "eslint-config-prettier": "^9.1.0",
    "prisma": "^5.9.1",
    "tsx": "^4.7.1"
  }
}
