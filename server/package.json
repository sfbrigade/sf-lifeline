{
  "type": "module",
  "name": "sf-lifeline-server",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "version": "1.0.0",
  "main": "app.js",
  "directories": {
    "test": "test"
  },
  "imports": {
    "#helpers/*": "./helpers/*",
    "#models/*": "./models/*",
    "#test/*": "./test/*"
  },
  "prisma": {
    "seed": "node prisma/seed"
  },
  "scripts": {
    "db:preview": "npx prisma studio",
    "db:reset": "prisma migrate reset --force",
    "dev": "prisma migrate dev && fastify start -w -l info -P app.js",
    "lint": "eslint --fix",
    "prisma:generate": "prisma generate",
    "start": "fastify start -l info app.js",
    "test": "node --test-concurrency 1 --test $(find test -name '*.test.js')",
    "test:db:reset": "DATABASE_URL=${DATABASE_URL}_test prisma migrate reset --force"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "^6.0.2",
    "@fastify/cookie": "^11.0.1",
    "@fastify/secure-session": "^8.1.0",
    "@fastify/sensible": "^6.0.1",
    "@fastify/static": "^8.0.3",
    "@fastify/swagger": "^9.4.0",
    "@getbigger-io/prisma-fixtures-cli": "^2.0.1",
    "@prisma/client": "^6.3.0",
    "@scalar/fastify-api-reference": "^1.25.76",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.5.0",
    "ejs": "^3.1.10",
    "email-templates": "^12.0.1",
    "fastify": "^5.1.0",
    "fastify-cli": "^7.0.1",
    "fastify-plugin": "^5.0.1",
    "fastify-zod-openapi": "^4.1.2",
    "http-status-codes": "^2.3.0",
    "luxon": "^3.5.0",
    "nodemailer": "^6.9.16",
    "uuid": "^11.0.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "c8": "^10.1.2",
    "eslint": "^9.17.0",
    "neostandard": "^0.12.0",
    "nock": "^14.0.0-beta.19",
    "nodemailer-mock": "^2.0.6",
    "prisma": "^6.3.0",
    "quibble": "^0.9.2"
  }
}
