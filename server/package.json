{
  "type": "module",
  "name": "sf-lifeline-server",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "version": "1.0.0",
  "main": "app.js",
  "directories": {
    "test": "test"
  },
  "prisma": {
    "seed": "tsx prisma/seed"
  },
  "scripts": {
    "db:reset": "npx prisma migrate reset --force",
    "dev": "npx prisma migrate dev && fastify start -w -l info -P app.js",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "start": "fastify start -l info app.js",
    "test": "node --test $(find test -name '*.test.js')",
    "test:db:reset": "DATABASE_URL=${DATABASE_URL}_test npx prisma migrate reset --force"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "^5.8.0",
    "@fastify/sensible": "^5.5.0",
    "@prisma/client": "^5.10.2",
    "dotenv": "^16.4.5",
    "fastify": "^4.26.1",
    "fastify-cli": "^6.1.1",
    "fastify-plugin": "^4.5.1",
    "nodemailer": "^6.9.12"
  },
  "devDependencies": {
    "c8": "^9.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "nodemailer-mock": "^2.0.4",
    "prisma": "^5.10.2",
    "quibble": "^0.9.1",
    "tsx": "^4.7.1"
  }
}
